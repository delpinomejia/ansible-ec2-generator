# ============================================================================
# Ansible EC2 Generator - Git Ignore Rules
# ============================================================================

# Ansible specific
vars/vars.yml                  # Never commit real credentials
*.retry                        # Ansible retry files
.ansible/                      # Ansible cache directory
ansible.cfg                    # Local ansible config (if any)

# AWS credentials and configs
.aws/                          # AWS CLI configuration
aws-credentials.yml            # AWS credential files
credentials.yml                # Any credential files

# Python
__pycache__/                   # Python cache
*.py[cod]                      # Compiled Python files
*$py.class                     # Python class files
*.so                           # Shared objects
.Python                        # Python build
build/                         # Build directory
develop-eggs/                  # Development eggs
dist/                          # Distribution directory
downloads/                     # Downloads
eggs/                          # Eggs directory
.eggs/                         # Eggs info
lib/                           # Library files
lib64/                         # 64-bit library files
parts/                         # Parts directory
sdist/                         # Source distribution
var/                           # Variable files
wheels/                        # Wheel files
pip-wheel-metadata/            # Pip wheel metadata
share/python-wheels/           # Shared python wheels
*.egg-info/                    # Egg info
.installed.cfg                 # Installation config
*.egg                          # Egg files
MANIFEST                       # Manifest file

# Virtual environments
venv/                          # Virtual environment
ENV/                           # Environment directory
env/                           # Environment
.venv/                         # Virtual environment
.ENV/                          # Environment
.env                           # Environment variables

# IDE and Editors
.vscode/                       # VS Code settings
.idea/                         # IntelliJ IDEA
*.swp                          # Vim swap files
*.swo                          # Vim swap files
*~                             # Backup files
.DS_Store                      # macOS directory metadata
Thumbs.db                      # Windows thumbnail cache

# Logs and temporary files
*.log                          # Log files
logs/                          # Log directory
*.tmp                          # Temporary files
*.temp                         # Temporary files

# OS specific
.DS_Store                      # macOS
.DS_Store?                     # macOS
._*                            # macOS
.Spotlight-V100                # macOS
.Trashes                       # macOS
ehthumbs.db                    # Windows
Thumbs.db                      # Windows
Desktop.ini                    # Windows

# CI/CD artifacts
.gitlab-ci-artifacts/          # GitLab CI artifacts
junit.xml                      # Test results
coverage.xml                   # Coverage reports

# Security and sensitive data
*.pem                          # Private keys
*.key                          # Private keys
*.crt                          # Certificates
*.crt.backup                   # Certificate backups
secrets.yml                    # Secret files
vault-password                 # Vault passwords
.vault_pass                    # Vault password files

# Backup files
*.bak                          # Backup files
*.backup                       # Backup files
*~                             # Backup files

# Documentation builds
docs/_build/                   # Sphinx build
site/                          # MkDocs build
